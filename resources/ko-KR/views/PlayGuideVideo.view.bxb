result-view {
  match: PlayGuideVideoResult(this)
  
  message {
    choose (Random) {
      template ("곧 가이드 영상이 실행됩니다. #{value(this)}"){
        speech ("곧 가이드 영상이 실행됩니다.")
      }
      template ("잠시 후 가이드 영상을 재생합니다."){
        speech ("잠시 후 가이드 영상을 재생합니다.")
      }
    }
  }

  app-launch {
    payload-uri ("#{value(this)}")
  }
}